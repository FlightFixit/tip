( function() {
    var scripts
    for( idx in scripts = [ '.../lib/jquery/js', '.../lib/mimis/jquery', 'mimis' ] ) {
        ( function() {
            var script = { get href() { scripts[ idx ] } }
            //script += { ... }
            script.__defineGetter__( 'elem', function() {
                var elem = document.createElement( 'script' )
                elem.setAttribute( 'type', 'application/javascript' )
                elem.setAttribute( 'href', script.href )
                elem.setAttributeNS( 'http://www.w3.org/1999/xlink', 'href', script.href )
                return elem
            } )

            var insPoint = document.documentElement.lastChild // Will fail when nested
            if( insPoint.nextSibling ) {
                insPoint.parentNode.insertBefore( elem, insPoint.nextSibling )
            } else {
                insPoint.parentNode.appendChild( elem )
            }
        } )()
    }


} )()
