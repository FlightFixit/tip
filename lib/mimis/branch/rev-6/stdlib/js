console.log( '▶.../lib/mimis/stdlib/jquery' )

;( function() {

    console.log( typeof mimis.load )
    console.log( mimis.base )

    mimis.load( mimis.base + 'stdlib/array/js' )
    mimis.load( mimis.base + 'stdlib/string/js' )
    mimis.load( mimis.base + 'stdlib/notify/js' )
    mimis.load( mimis.base + 'stdlib/namespace/js' )

    if( mimis.inDOM ) {
	mimis.load( mimis.base + 'stdlib/applet/jquery')  // Browser-JVM bridge'
    }
    
    mimis.load( mimis.base + 'stdlib/hashtree/js' )
} )()

console.log( '◀.../lib/mimis/stdlib/jquery' )
