//!/bin/js

( function() {
    var lib = '.../lib/mimis/js'
    
    print( environment.OSTYPE )
    
    return

    if( environment.OSTYPE == 'cygwin' ) {
        lib = runCommand( 'cygpath', '-w', lib )
    }
    
    print( lib )
    
    //load( lib )
    
    //console.log( mimis.load( eson ) )
} )()
