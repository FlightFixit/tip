//!/bin/js

( function() {
    print( readFile( 'eson' ) )

    if( typeof environment !== 'undefined'
        && environment[ 'os.emulator' ] == 'cygwin' ) {
        load( 'C:\\Program Files\\Mimis\\js' )
    } else {
        load( '.../lib/mimis/js' )
    }

    console.log( mimis.load( 'eson' ) )
} )()
