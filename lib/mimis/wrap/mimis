( function() { return function() {
    var out = {}
    var args = Array.prototype.slice.call( arguments )

    args.each( function( obj ) {
        try {
            for( prop in obj ) {
                ( function() {
                    var attr = prop
                    var getter = function() { return obj[ attr ] }
                    var setter = function( val ) { return obj[ attr ] = val }
                    out.__defineGetter__( prop, getter )
                    out.__defineSetter__( prop, setter )
                } )()
            }
        } catch( e ) {
            console.error( e )
        }
    } )
    return out
} } )()
