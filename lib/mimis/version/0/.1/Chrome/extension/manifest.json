{
  "name" : "Mïmis",
  "description" : "Browser-based access to the Mïmis peer-to-peer network",
  "version" : "0.3",
  "background_page" : "background/xhtml.xhtml",
  "icons" : {
      "16" : "icon/16/png",
      "48" : "icon/48/png",
      "128" : "icon/128/png"
  },
  "update_url" : "http://mimis.dhappy.org/Chrome/extension/gupdate/xml",
  "options_page": "options/xhtml.xhtml",
  "omnibox" : { "keyword" : "t" },
  "browser_action" : {
      "default_icon" : "icon/16/png",
      "default_title" : "Search Mïmis",
      "default_popup" : "popup/xhtml.xhtml"
  },
  "permissions" : [
      "notifications",
      "http://dhappy.dyndns.org/*/",
      "http://localhost/*/",
      "tabs"
  ],
  "unused_permissions" : [
      "http://*/*"
  ]
}
