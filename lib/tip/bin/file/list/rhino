var DOMService = com.sun.java.browser.dom.DOMService
var DOMAction = com.sun.java.browser.dom.DOMAction
var DOMAccessor = com.sun.java.browser.dom.DOMAccessor
var DOMUnsupportedException = com.sun.java.browser.dom.DOMUnsupportedException
var DOMAccessException = com.sun.java.browser.dom.DOMAccessException

var InputStream = java.io.InputStream
var InputStream = java.io.InputStreamReader
var BufferedReader = java.io.BufferedReader

var service = null

eval( 'print( "Entering: /bin/file/list/rhino" )' )
print( "typeof load = " + typeof load )

if( typeof load == 'undefined' ) {
    function load( resource ) {
	var stream = null
	var reader = null
	var line = ""
	try {
	    try {
		stream = hostApplet.getResourceAsStream( resource )
		if( stream == null ) {
		    print( "Could not get: " + resource );
		} else {
		    reader = new BufferedReader( new InputStreamReader( stream ) )
		    eval( reader )
		}
	    } finally {
		if( reader != null ) {
		    reader.close()
		}
		if( stream != null ) {
		    stream.close()
		}
	    }
	} catch( e ) {
	    print( "Error: " + e + "\n" )
	}
    	print( "here: " + resource )
    }
}
print( "typeof load = " + typeof load )
load( '/lib/jquery/js' )

try {
    service = DOMService.getService( hostApplet )
    var doc = service.invokeAndWait( new DOMAction( {
	run : function( accessor ) {
	    var doc = accessor.getDocument( hostApplet )
	    return doc.getTitle()
	}
    } ) )
    print( "Embedded in page w/ title: " + title )
} catch( e ) {
    print( e )
}