module('$.fn.props');

test('Existence', function () {
    equals(typeof $.fn.props, 'function');
});

test('Object', function () {
    var obj = { x: 10 };

    var props = $(obj).props();
    equals(props.x.value, 10);
    equals(props.y, undefined);

    var props = $(obj).props([ 'x', 'y' ]);
    equals(props.x.value, 10);
    equals(props.y.value, undefined);

    props.y.value = 15;
    equals(obj.y, 15);
});
